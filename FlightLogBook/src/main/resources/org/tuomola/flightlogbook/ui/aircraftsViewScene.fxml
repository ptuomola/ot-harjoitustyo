<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.tuomola.flightlogbook.ui.AircraftsViewController">
   <children>
      <VBox prefHeight="527.0" prefWidth="497.0">
         <children>
            <TableView fx:id="table" prefHeight="477.0" prefWidth="856.0">
              <columns>
                  <TableColumn fx:id="identifier" editable="false" prefWidth="108.0" style="-fx-alignment: CENTER;" text="Registration" />
                  <TableColumn fx:id="type" prefWidth="117.0" style="-fx-alignment: CENTER;" text="Type" />
                  <TableColumn fx:id="hoursFlown" prefWidth="115.0" style="-fx-alignment: CENTER;" text="Total time flown" />
                  <TableColumn fx:id="lastFlightDate" minWidth="0.0" prefWidth="108.0" style="-fx-alignment: CENTER;" text="Last flight" />
              </columns>
            </TableView>
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
                <Button defaultButton="true" mnemonicParsing="false" onAction="#handleOkButtonAction" text="OK" />
              </buttons>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </ButtonBar>
         </children>
         <StackPane.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </StackPane.margin>
      </VBox>
   </children>
</StackPane>
