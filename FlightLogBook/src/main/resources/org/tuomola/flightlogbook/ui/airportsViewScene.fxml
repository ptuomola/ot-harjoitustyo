<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.tuomola.flightlogbook.ui.AirportsViewController">
   <children>
      <VBox prefHeight="527.0" prefWidth="572.0">
         <children>
            <TableView fx:id="table" prefHeight="477.0" prefWidth="856.0">
              <columns>
                  <TableColumn fx:id="code" editable="false" prefWidth="91.0" style="-fx-alignment: CENTER;" text="Code" />
                  <TableColumn fx:id="name" prefWidth="151.0" style="-fx-alignment: CENTER;" text="Name" />
                  <TableColumn fx:id="lastVisit" prefWidth="110.0" style="-fx-alignment: CENTER;" text="Last visit" />
                  <TableColumn fx:id="numDepartures" minWidth="0.0" prefWidth="109.0" style="-fx-alignment: CENTER;" text="# departures" />
                  <TableColumn fx:id="numArrivals" minWidth="0.0" prefWidth="99.0" style="-fx-alignment: CENTER;" text="# arrivals" />
              </columns>
            </TableView>
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
                <Button defaultButton="true" mnemonicParsing="false" onAction="#handleOkButtonAction" text="OK" />
              </buttons>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </ButtonBar>
         </children>
         <StackPane.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </StackPane.margin>
      </VBox>
   </children>
</StackPane>
